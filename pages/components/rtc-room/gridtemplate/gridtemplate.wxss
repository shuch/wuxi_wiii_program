.bg {
  position: absolute;
  left: 0;
  bottom: 0;
  background-image: url(http://skyforest.static.elab-plus.com/wepy_pro/bg-xcx.jpg);
  background-color: #333;
  background-repeat:no-repeat;
  background-size: cover;
  width:100vw;
  height: 100vh;
}
.character {
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 16px;
  color: #999;
  width: 100%;
  height: 10vw;
  line-height: 10vw;
  text-align: left;
}
.loading {
  position: absolute;
  width: 100%;  
  height: 100%; 
}
.loading cover-image {
  position: absolute;
  top: calc(50% - 15px);
  left: calc(50% - 15px);
  width: 30px;
  height: 30px;
}
